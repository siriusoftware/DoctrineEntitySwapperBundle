{
    "name": "sirius-software/doctrine-entity-swapper-bundle",
    "description": "This bundle allows developers to change doctrine entity and repository classes when interfaces used for enity definition. Ideal for multi-tenant systems",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SiriusSoftware\\DoctrineEntitySwapperBundle\\": ""
        }
    },
    "authors": [
        {
            "name": "Sirius Software"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0|^8.1",
        "symfony/config": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "symfony/event-dispatcher": "^5.3|^6.0",
        "symfony/framework-bundle": "^5.0|^6.0",
        "doctrine/orm": "*",
        "doctrine/doctrine-bundle": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "vimeo/psalm": "4.12.0",
        "psalm/plugin-symfony": "^3.1",
        "psalm/plugin-phpunit": "^0.17.0",
        "symplify/easy-coding-standard": "^11.1",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11"
    }
}
